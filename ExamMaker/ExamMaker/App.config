<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <connectionStrings>
    <add name="ExamMakerDbContext" connectionString="Data Source=(local);Initial Catalog=EXAMMAKERD;Integrated Security=True;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="documentExporterInstance" value="ExamMaker.BusinessObjects.MsWordExporter"/>
    <add key="answerkeyDefaultFilename" value="AnswerKey_{0}"/>
    <add key="answerkeySaveFolder" value=""/>
    <add key="examDefaultFilename" value="Exam_{0}"/>
    <add key="examSaveFolder" value=""/>
    <add key="examTemplatePath" value="Templates\ExamHeaderTemplate.docx"/>
    <add key="fillInTheBlanksInstructions" value="Fill in the blanks with the correct answer."/>
    <add key="identificationInstructions" value="Identify what is being described."/>
    <add key="essayInstructions" value="Answer the questions briefly."/>
    <add key="multipleChoiceInstructions" value="Encircle the letter of the best answer."/>
    <!--
    Exam Format Order Values:
      FillInTheBlanks
      Identification
      Essay
      MultipleChoice
    -->
    <add key="examFormatNamespace" value="ExamMaker.Formatters"/>
    <add key="examFormatOrder" value="FillInTheBlanks,Identification,Essay,MultipleChoice"/>
    <add key="passwordAndNameFieldsMinLength" value="6"/>
  </appSettings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
</configuration>